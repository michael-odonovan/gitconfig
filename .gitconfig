[user]
	name = Micheal O'Donovan 
	email = mikeodonovan3@gmail.com
	user = michael-odonovan
[init]
	defaultBranch = main
[alias]
	llog = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date=relative --branches
[pull]
	rebase = false
[commit]
	template = ~/.gitmessage
[credential]
	helper = cache
[core]
	editor = lvim
